{
    "manifest_version": 2,
    "name": "urbandictionary-omnibox",
    "description": "Urban Dictionary search suggestions in your omnibox (address bar). The keyword is `u`.",
    "version": "0.0.1",
    "omnibox": { "keyword" : "u" },
    "permissions": [
        "tabs",
	"*://api.urbandictionary.com/*"
    ],
    "background": {
	"persistent": false,
	"page": "event_page.html"
    },
    "icons": {
	"128": "icon128.png"
    }
}
