{
    "manifest_version": 2,
    "name": "urbandictionary-omnibox",
    "description": "Urbandictionary search suggestions in your omnibox (address bar).",
    "version": "0.0.1",
    "omnibox": { "keyword" : "ud" },
    "permissions": [
        "tabs",
	"https://api.urbandictionary.com/*"
    ],
    "background": {
	"persistent": false,
	"scripts": ["event_page.js"]
    },
    "icons": {
	"128": "icon128.png"
    }
}
